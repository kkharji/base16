        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#838085" //3
            }
        },
        {
            "name": "Variables",
            "scope": [
