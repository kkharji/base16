[global]
frame_color = "#545454"
separator_color = "#545454"

[base16_low]
    msg_urgency = low
    background = "#e8e8e8"
    foreground = "#c0c0c0"

[base16_normal]
    msg_urgency = normal
    background = "#d4d4d4"
    foreground = "#545454"

[base16_critical]
    msg_urgency = critical
    background = "#800000"
    foreground = "#2a2a2a"
