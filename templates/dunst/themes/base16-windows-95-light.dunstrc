[global]
frame_color = "#545454"
separator_color = "#545454"

[base16_low]
    msg_urgency = low
    background = "#e0e0e0"
    foreground = "#a8a8a8"

[base16_normal]
    msg_urgency = normal
    background = "#c4c4c4"
    foreground = "#545454"

[base16_critical]
    msg_urgency = critical
    background = "#a80000"
    foreground = "#2a2a2a"
