! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #d0dad0, #d0dad0, #e0e0e0

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #d0dad0, #000000, #d0dad0, #d0dad0, #c4d9c4
rofi.color-active:  #d0dad0, #007300, #d0dad0, #d0dad0, #007300
rofi.color-urgent:  #d0dad0, #840000, #d0dad0, #d0dad0, #840000

! Set the desired separator style
rofi.separator-style: solid
