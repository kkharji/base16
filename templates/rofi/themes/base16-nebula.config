! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #414f60, #414f60, #22273b

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #414f60, #a4a6a9, #414f60, #414f60, #8dbdaa
rofi.color-active:  #414f60, #4d6bb6, #414f60, #414f60, #4d6bb6
rofi.color-urgent:  #414f60, #777abc, #414f60, #414f60, #777abc

! Set the desired separator style
rofi.separator-style: solid
