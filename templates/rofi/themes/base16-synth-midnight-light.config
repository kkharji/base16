! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #cfd1d2, #cfd1d2, #dddfe0

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #cfd1d2, #28292a, #cfd1d2, #cfd1d2, #050608
rofi.color-active:  #cfd1d2, #03aeff, #cfd1d2, #cfd1d2, #03aeff
rofi.color-urgent:  #cfd1d2, #b53b50, #cfd1d2, #cfd1d2, #b53b50

! Set the desired separator style
rofi.separator-style: solid
