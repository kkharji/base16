! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #2a261c, #2a261c, #1c1810

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #2a261c, #736e55, #2a261c, #2a261c, #f8f5de
rofi.color-active:  #2a261c, #489bf0, #2a261c, #2a261c, #489bf0
rofi.color-urgent:  #2a261c, #e35142, #2a261c, #2a261c, #e35142

! Set the desired separator style
rofi.separator-style: solid
