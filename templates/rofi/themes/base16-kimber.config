! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #313131, #313131, #222222

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #313131, #DEDEE7, #313131, #313131, #FFFFE6
rofi.color-active:  #313131, #537C9C, #313131, #313131, #537C9C
rofi.color-urgent:  #313131, #C88C8C, #313131, #313131, #C88C8C

! Set the desired separator style
rofi.separator-style: solid
