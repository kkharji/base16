! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #333333, #333333, #111111

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #333333, #bbbbbb, #333333, #333333, #ffffff
rofi.color-active:  #333333, #0074d9, #333333, #333333, #0074d9
rofi.color-urgent:  #333333, #ff4136, #333333, #333333, #ff4136

! Set the desired separator style
rofi.separator-style: solid
