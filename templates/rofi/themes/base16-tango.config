! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #8ae234, #8ae234, #2e3436

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #8ae234, #d3d7cf, #8ae234, #8ae234, #eeeeec
rofi.color-active:  #8ae234, #3465a4, #8ae234, #8ae234, #3465a4
rofi.color-urgent:  #8ae234, #cc0000, #8ae234, #8ae234, #cc0000

! Set the desired separator style
rofi.separator-style: solid
