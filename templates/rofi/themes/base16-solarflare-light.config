! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #E8E9ED, #E8E9ED, #F5F7FA

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #E8E9ED, #586875, #E8E9ED, #E8E9ED, #18262F
rofi.color-active:  #E8E9ED, #33B5E1, #E8E9ED, #E8E9ED, #33B5E1
rofi.color-urgent:  #E8E9ED, #EF5253, #E8E9ED, #E8E9ED, #EF5253

! Set the desired separator style
rofi.separator-style: solid
