! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #CCD4D3, #CCD4D3, #E9F1EF

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #CCD4D3, #385156, #CCD4D3, #CCD4D3, #D2FAFF
rofi.color-active:  #CCD4D3, #39AAC9, #CCD4D3, #CCD4D3, #39AAC9
rofi.color-urgent:  #CCD4D3, #CF432E, #CCD4D3, #CCD4D3, #CF432E

! Set the desired separator style
rofi.separator-style: solid
