! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #1f1d2e, #1f1d2e, #191724

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #1f1d2e, #e0def4, #1f1d2e, #1f1d2e, #c5c3ce
rofi.color-active:  #1f1d2e, #9ccfd8, #1f1d2e, #1f1d2e, #9ccfd8
rofi.color-urgent:  #1f1d2e, #e2e1e7, #1f1d2e, #1f1d2e, #e2e1e7

! Set the desired separator style
rofi.separator-style: solid
