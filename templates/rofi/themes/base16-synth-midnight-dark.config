! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #1a1b1c, #1a1b1c, #050608

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #1a1b1c, #c1c3c4, #1a1b1c, #1a1b1c, #dddfe0
rofi.color-active:  #1a1b1c, #03aeff, #1a1b1c, #1a1b1c, #03aeff
rofi.color-urgent:  #1a1b1c, #b53b50, #1a1b1c, #1a1b1c, #b53b50

! Set the desired separator style
rofi.separator-style: solid
