! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #f8e2e7, #f8e2e7, #feedf3

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #f8e2e7, #564448, #f8e2e7, #f8e2e7, #33292b
rofi.color-active:  #f8e2e7, #006e93, #f8e2e7, #f8e2e7, #006e93
rofi.color-urgent:  #f8e2e7, #df2d52, #f8e2e7, #f8e2e7, #df2d52

! Set the desired separator style
rofi.separator-style: solid
