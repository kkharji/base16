! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #333b3d, #333b3d, #232629

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #333b3d, #f8f8f2, #333b3d, #333b3d, #fcfcfc
rofi.color-active:  #333b3d, #00a6fb, #333b3d, #333b3d, #00a6fb
rofi.color-urgent:  #333b3d, #f11235, #333b3d, #333b3d, #f11235

! Set the desired separator style
rofi.separator-style: solid
