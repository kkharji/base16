! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #1D494E, #1D494E, #0e3c46

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #1D494E, #C7DBDD, #1D494E, #1D494E, #D2FAFF
rofi.color-active:  #1D494E, #46bddd, #1D494E, #1D494E, #46bddd
rofi.color-urgent:  #1D494E, #fb6953, #1D494E, #1D494E, #fb6953

! Set the desired separator style
rofi.separator-style: solid
