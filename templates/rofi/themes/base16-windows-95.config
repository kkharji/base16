! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #1C1C1C, #1C1C1C, #000000

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #1C1C1C, #a8a8a8, #1C1C1C, #1C1C1C, #fcfcfc
rofi.color-active:  #1C1C1C, #5454fc, #1C1C1C, #1C1C1C, #5454fc
rofi.color-urgent:  #1C1C1C, #fc5454, #1C1C1C, #1C1C1C, #fc5454

! Set the desired separator style
rofi.separator-style: solid
