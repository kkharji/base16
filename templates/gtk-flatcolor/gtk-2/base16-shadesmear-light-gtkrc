# In file gtk-2.0/gtkrc, delete "gtk-color-scheme" including everything between quotes (don't delete gtk-auto-mnemonics) and inject this.
gtk-color-scheme = "bg_color:#dbdbdb
color0:#dbdbdb
text_color:#232323
selected_bg_color:#c0c0c0
selected_fg_color:#232323
tooltip_bg_color:#dbdbdb
tooltip_fg_color:#232323
titlebar_bg_color:#dbdbdb
titlebar_fg_color:#232323
menu_bg_color:#dbdbdb
menu_fg_color:#232323
link_color:#c0c0c0"
